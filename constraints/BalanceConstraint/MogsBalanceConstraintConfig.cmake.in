set(MogsBalanceConstraint_LIBRARIES @MogsBalanceConstraint_LIBRARIES@ @MogsCore_LIBRARIES@)

SET(MogsBalanceConstraint_FLAGS @MogsBalanceConstraint_FLAGS@ @MogsCore_FLAGS@)
