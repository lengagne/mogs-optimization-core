set(MogsNlpBAD_1_4_LIBRARIES @MogsNlpBAD_1_4_LIBRARIES@ @MogsCore_LIBRARIES@)

SET(MogsNlpBAD_1_4_FLAGS @MogsNlpBAD_1_4_FLAGS@ @MogsCore_FLAGS@)
